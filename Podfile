# Podfile

# This tells CocoaPods we want to target iOS versions 8.0 and above.
platform :ios, '8.0'
use_frameworks!

# Supress any warnings from Pods we install if you're getting a lot of noise from them.

inhibit_all_warnings!

target ‘Flick’ do

# This pulls in the latest version of AFNetworking >= 2.6 but less than 3.0.
pod 'AFNetworking', '~> 2.6'

# A pod that shows a loading search while fetching data from internet
pod 'MBProgressHUD'

end